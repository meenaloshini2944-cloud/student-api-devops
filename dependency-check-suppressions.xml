<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">

    <!-- Suppression entries must use REAL CVE IDs (CVE-YYYY-NNNN).
         Placeholders like CVE-2022-XXXX will break Dependency-Check. -->

    <!-- Mocha: dev-only testing dependency
         Add a real <cve> only after triage confirms false-positive / not-applicable. -->
    <suppress>
        <notes>
            Mocha is a development-only dependency used for testing.
            Not included in production runtime image (--omit=dev).
        </notes>
        <packageUrl regex="true">^pkg:npm/mocha@.*</packageUrl>
        <!-- <cve>CVE-2022-12345</cve> -->
    </suppress>

    <!-- Glob: transitive build/test dependency
         Add a real <cve> only after triage. -->
    <suppress>
        <notes>
            Glob is a transitive dependency used during build/test only.
            Not included in production Docker image (--omit=dev).
        </notes>
        <packageUrl regex="true">^pkg:npm/glob@.*</packageUrl>
        <!-- <cve>CVE-2021-12345</cve> -->
    </suppress>

</suppressions>
