{
  "name": "student-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node src/server.js",
    "sbom": "cyclonedx-npm --output-file sbom.json --output-format json",
    "test": "npm run test:ci",
    "test:ci": "nyc --reporter=text --reporter=lcov --reporter=html --check-coverage --lines=80 --functions=80 --branches=70 --statements=80 mocha \"tests/**/*.test.js\" --timeout 15000 --reporter mocha-junit-reporter --reporter-options mochaFile=reports/junit/mocha-junit.xml",
    "test:html": "mocha \"tests/**/*.test.js\" --timeout 15000 --reporter mochawesome --reporter-options reportDir=reports/mochawesome,reportFilename=mochawesome,html=true,json=true,quiet=true"
  },
  "dependencies": {
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1"
  },
  "overrides": {
  "cross-spawn": "7.0.5",
  "glob": "11.1.0",
  "tar": "7.5.7"
},
  "devDependencies": {
    "mocha": "^10.0.0",
    "chai": "^4.3.0",
    "supertest": "^7.2.2",
    "nyc": "^15.1.0",
    "mocha-junit-reporter": "^2.2.0",
    "mochawesome": "^7.1.3",
    "@cyclonedx/cyclonedx-npm": "^1.0.0"
  }
}
